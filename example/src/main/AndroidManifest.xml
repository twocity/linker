<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.twocities.linker.example">

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
      android:name="me.twocities.linker.example.ExampleApp"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".ProductActivity"/>
    <activity android:name=".JavaActivity"/>
    <activity android:name=".OrderActivity"/>
    <activity android:name=".SimpleBrowserActivity"/>
    <activity android:name=".FallbackActivity"/>
  </application>

</manifest>